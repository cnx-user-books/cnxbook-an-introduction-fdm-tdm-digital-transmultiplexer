                      <m:mn>1</m:mn>
                    </m:mrow>
                  </m:msub>
                  <m:msub>
                    <m:mi>H</m:mi>
                    <m:mn>0</m:mn>
                  </m:msub>
                  <m:mo>,</m:mo>
        
                </m:mrow>
              </m:mtd>
<m:mtd columnalign="left">
          <m:msub>
                    <m:mi>k</m:mi>
                    <m:mrow>
