                    <m:mi>k</m:mi>
                  </m:msub>
                  <m:msub>
                    <m:mover>
                      <m:mi>H</m:mi>
                      <m:mo>¯</m:mo>
                    </m:mover>
                    <m:mi>e</m:mi>
                  </m:msub>
                  <m:mo>,</m:mo>
                  <m:mspace width="4pt"/>
                  <m:mrow>
                    <m:mtext>a</m:mtext>
                    <m:mi>n</m:mi>
                    <m:mi>d</m:mi>
                  </m:mrow>
                </m:mrow>
              </m:mtd>
            </m:mtr>
            <m:mtr>
              <m:mtd>
                <m:mrow>
                  <m:mi>y</m:mi>
                  <m:mo>(</m:mo>
                  <m:mi>k</m:mi>
                  <m:mo>:</m:mo>
                  <m:mi>k</m:mi>
                  <m:mspace width="4pt"/>
                  <m:mi>o</m:mi>
                  <m:mi>d</m:mi>
                  <m:mi>d</m:mi>
                  <m:mo>)</m:mo>
                </m:mrow>
              </m:mtd>
              <m:mtd>
                <m:mo>=</m:mo>
              </m:mtd>
              <m:mtd>
