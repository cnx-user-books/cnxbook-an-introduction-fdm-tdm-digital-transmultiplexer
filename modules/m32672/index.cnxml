                  <m:mo>,</m:mo>
</m:mtd>        
    </m:mtr>
            <m:mtr>
              <m:mtd columnalign="left">
                  <m:mi>y</m:mi>
                  <m:mrow>
                    <m:mo>(</m:mo>
                    <m:mi>k</m:mi>
                    <m:mo>+</m:mo>
                    <m:mn>2</m:mn>
                    <m:mo>)</m:mo>
                  </m:mrow>
</m:mtd>
<m:mtd columnalign="left">                  
<m:mo>=</m:mo>
</m:mtd>
<m:mtd columnalign="left">       
           <m:mo>-</m:mo>
                  <m:mspace width="4pt"/>
                  <m:msub>
                    <m:mi>X</m:mi>
                    <m:mrow>
                      <m:mi>k</m:mi>
                      <m:mo>+</m:mo>
                      <m:mn>2</m:mn>
                    </m:mrow>
                  </m:msub>
                  <m:msub>
                    <m:mi>H</m:mi>
